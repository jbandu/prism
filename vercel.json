{
  "version": 2,
  "builds": [
    {
      "src": "prism-web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "prism-web/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/enhance",
      "schedule": "0 2 * * 1"
    }
  ]
}
